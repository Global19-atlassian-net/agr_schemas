{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Global IDs",
  "id": "wildtypeExpressionTermIdentifiers.json#",
  "description": "Group of ids that represent where a gene is expressed.",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "cellularComponentTermId": {
    	    "$ref": "../globalId.json#/properties/globalId",
	    "description": "the id that represents a part, or the entire, expression statement - an ontology id."   
    },
    "cellularComponentAgrSlimTerms":{
	"type": "array",
	"items": {
		 "type":"string",
		 "enum": ["GO:0005576",
		 	  "GO:0005737",
			  "GO:0005856",
			  "GO:0005739",
			  "GO:0005634",
			  "GO:0005694",
			  "GO:0016020",
