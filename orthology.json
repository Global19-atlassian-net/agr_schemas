{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Orthology",
  "description": "orthology objects.",
  "id": "orthology.json",
  "type": "object",
  "required": [ "metaData", "data" ],
  "properties": {
    "isBestScore": { "enum": ["Yes","No"],
                     "description": "Diopt schema highlights the best pair."
    },
    "isBestRevScore": { "enum": ["Yes","No"],
                     "description": "Diopt schema highlights the best pair."
    },
    "gene1": {
      "type": "string",
      "description": "The first gene of the pair."
    },
    "gene1DataProvider": {
      "$ref" : "dataProvider.json#/properties/dataProvider",
      "description":"The provider of the original gene id."
    },
    "gene2": {
      "type": "string",
      "description": "The second gene of the pair."
    },
    "gene1DataProvider": {
      "$ref" : "dataProvider.json#/properties/dataProvider",
      "description":"The provider of the original gene id."
    },
    "predictionMethod": {
      "enum":["Compara","Panther","OrthoMCL","ZFIN Curated Orthology","InParanoid","TreeFam","RoundUp","OrthoDB"],
      "description":"Source of the match."
    },
    "confidence": {
      "enum":["moderate","high","low"],
      "description":"Confidence of match"
    }
  }
}
