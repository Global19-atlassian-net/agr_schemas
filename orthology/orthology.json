{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Orthology",
  "description": "orthology objects.",
  "id": "orthology.json",
  "type": "object",
  "required": [ "isBestScore", "isBestRevScore", "gene1", "gene2", "gene1UsedSymbol", "gene2UsedSymbol", "gene1DataProvider", "gene2DataProvider", "confidence" ],
  "properties": {
    "isBestScore": { "enum": ["Yes","No"],
                     "description": "DIOPT schema highlights the best pair."
    },
    "isBestRevScore": { "enum": ["Yes","No"],
                     "description": "DIOPT schema highlights the best reverse pair."
    },
    "gene1": {
      "type": "string",
      "description": "The first gene of the pair."
    },
    "gene1Symbol": {
      "type": "string",
      "description": "The symbol for gene 1."
    },
    "gene1SymbolAsId": { "enum": ["Yes","No"],
      "description": "Did we use a gene symbol as the ID for gene 1? This is true if the species_specific_geneid was missing in DIOPT."
    },
    "gene1DataProvider": {
      "$ref" : "../dataProvider.json#/properties/dataProvider",
      "description":"The provider of the original gene id."
    },
    "gene2": {
      "type": "string",
      "description": "The second gene of the pair."
    },
    "gene2Symbol": {
      "type": "string",
      "description": "The symbol for gene 2."
    },
    "gene2SymbolAsId": { "enum": ["Yes","No"],
      "description": "Did we use a gene symbol as the ID for gene 2? This is true if the species_specific_geneid was missing in DIOPT."
    },
    "gene1DataProvider": {
      "$ref" : "../dataProvider.json#/properties/dataProvider",
      "description":"The provider of the original gene id."
    },
    "predictionMethodsMatched": {
      "type": "array",
      "items": {
        "type": "string",
        "value": { 
          "enum": ["OMA","Panther","Inparanoid","OrthoDB","Isobase","TreeFam","RoundUp","Phylome","orthoMCL","ZFIN","HGNC","Homologene","Compara","eggNOG"]
        }
      },
        "description": "Source of the orthology match.",
        "uniqueItems": true
    },
    "predictionMethodsNotMatched": {
      "type": "array",
      "items": {
        "type": "string",
        "value": { 
          "enum": ["OMA","Panther","Inparanoid","OrthoDB","Isobase","TreeFam","RoundUp","Phylome","orthoMCL","ZFIN","HGNC","Homologene","Compara","eggNOG"]
        }
      },
        "description": "Prediction methods that were not matched.",
        "uniqueItems": true
    },
    "predictionMethodsNotCalled": {
      "type": "array",
      "items": {
        "type": "string",
        "value": { 
          "enum": ["OMA","Panther","Inparanoid","OrthoDB","Isobase","TreeFam","RoundUp","Phylome","orthoMCL","ZFIN","HGNC","Homologene","Compara","eggNOG"]
        }
      },
        "description": "Prediction methods that were not called.",
        "uniqueItems": true
    },
    "confidence": {
      "enum":["moderate","high","low"],
      "description":"Confidence of orthology match."
    }
  }
}
